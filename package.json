{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader to change the world",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "browserify": "browserify --ignore crypto --transform [ babelify --extensions .js,.ts ] --plugin [ tsify ]",
    "build": "npm-run-all -p build:css build:app-js build:worker-js -s dist",
    "build:app-js": "npm run browserify -- --outfile dist/app.js src/js/app.js && npm run uglify -- --output dist/app.min.js dist/app.js",
    "build:css": "lessc src/less/index.less dist/index.css && cleancss -o dist/index.min.css dist/index.css ",
    "build:worker-js": "npm run browserify -- --outfile dist/worker.js src/js/worker.ts && npm run uglify -- --output dist/worker.min.js dist/worker.js",
    "clean": "rimraf dist app/chrome/assets",
    "dist": "ncp dist app/chrome/assets",
    "httpserver": "http-server dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglify": "uglifyjs --compress warnings=false --mangle",
    "watch": "npm-run-all -p watch:css watch:app-js watch:worker-js watch:dist",
    "watch:app-js": "npm run watchify -- --outfile dist/app.js src/js/app.js",
    "watch:css": "watch 'lessc src/less/index.less dist/index.css' src/less",
    "watch:worker-js": "npm run watchify -- --outfile dist/worker.js src/js/worker.ts",
    "watch:dist": "watch 'npm run dist' dist",
    "watchify": "watchify --debug --verbose --ignore crypto --transform [ babelify --extensions .js,.ts ] --plugin [ tsify ]"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "http-server": "^0.8.5",
    "less": "^2.5.3",
    "linq": "^3.0.5",
    "localforage": "^1.3.0",
    "ncp": "^2.0.0",
    "node-uuid": "^1.4.3",
    "npm-run-all": "^1.2.12",
    "qs": "^6.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "reflect-metadata": "^0.1.2",
    "rimraf": "^2.4.3",
    "rx": "^4.0.6",
    "tsify": "^0.12.2",
    "typescript": "^1.6.2",
    "uglify-js": "^2.5.0",
    "watch": "^0.16.0",
    "watchify": "^3.6.0",
    "whatwg-fetch": "^0.10.0"
  }
}
