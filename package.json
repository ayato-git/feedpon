{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader for me",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "npm-run-all -p build:css build:fonts build:js",
    "build:css": "lessc --clean-css src/less/index.less app/chrome/css/index.css",
    "build:fonts": "ncp node_modules/font-awesome/fonts app/chrome/fonts",
    "build:js": "webpack --optimize-dedupe --optimize-minimize",
    "clean": "rimraf app/chrome/{js,css,fonts}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:css watch:fonts watch:js",
    "watch:css": "watch 'lessc --verbose src/less/index.less app/chrome/css/index.css' src/less",
    "watch:fonts": "watch 'npm run build:fonts' node_modules/font-awesome/fonts",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "classnames": "^2.2.0",
    "clean-css": "^3.4.8",
    "font-awesome": "^4.4.0",
    "iconv-lite": "^0.4.13",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-plugin-clean-css": "^1.5.1",
    "linq": "^3.0.5",
    "localforage": "^1.3.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.2.12",
    "querystring": "^0.2.0",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-dom": "^0.14.3",
    "reflect-metadata": "^0.1.2",
    "regenerator": "^0.8.42",
    "rx": "^4.0.6",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.3",
    "watch": "^0.16.0",
    "webpack": "^1.12.9",
    "whatwg-fetch": "^0.10.0"
  }
}
