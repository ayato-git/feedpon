{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "A feed reader for me",
  "author": "Shota Nozaki",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "npm-run-all -p build:css build:js",
    "build:css": "postcss -o public/css/index.css resources/css/index.css",
    "build:js": "webpack",
    "clean": "rm -f -r public/{js,css}",
    "lint": "tslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server -c-1 ./public",
    "watch": "npm-run-all -p watch:css watch:js",
    "watch:css": "postcss -w -o public/css/index.css resources/css/index.css",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "dependencies": {
    "@emonkak/enumerable": "^2.0.0",
    "classnames": "^2.2.5",
    "intl-relativeformat": "^1.3.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.4",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.46",
    "@types/classnames": "2.2.0",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.11",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^3.0.4",
    "@types/redux-logger": "^3.0.0",
    "@types/text-encoding": "0.0.30",
    "autoprefixer": "^7.1.1",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "cssnano": "^3.10.0",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^4.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-csso": "^2.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^10.0.0",
    "postcss-simple-vars": "^4.0.0",
    "ts-loader": "^2.0.1",
    "tslint": "^5.0.0",
    "typescript": "^2.1.6",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.0",
    "webpack": "^2.2.1"
  }
}
