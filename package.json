{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader for me",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -p build:css build:fonts build:js",
    "build:css": "postcss -c postcss.production.config.json -o app/chrome/css/index.css src/css/index.css",
    "build:fonts": "cp -r node_modules/font-awesome/fonts app/chrome/fonts",
    "build:js": "webpack",
    "clean": "rm -fr app/chrome/{js,css,fonts}",
    "lint": "eslint src && tslint src/**/*.ts",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:css watch:fonts watch:js",
    "watch:css": "postcss -c postcss.config.json -w -o app/chrome/css/index.css src/css/index.css",
    "watch:fonts": "watch 'npm run build:fonts' node_modules/font-awesome/fonts",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "dependencies": {
    "@emonkak/enumerable": "0.0.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.6.3",
    "in-viewport": "^3.4.1",
    "localforage": "^1.4.2",
    "querystring": "^0.2.0",
    "react": "^15.2.1",
    "react-addons-pure-render-mixin": "^15.2.1",
    "react-addons-update": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "react-waypoint": "^3.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.10",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "cssnano": "^3.7.3",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "npm-run-all": "^1.8.0",
    "postcss-cli": "^2.5.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "watch": "^0.16.0",
    "webpack": "^1.13.1"
  }
}
