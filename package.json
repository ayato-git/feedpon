{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "A feed reader for me",
  "author": "Shota Nozaki",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "npm-run-all -p build:css build:js -s build:cordova",
    "build:cordova": "cordova prepare",
    "build:css": "postcss -o ./www/css/index.css ./res/css/index.css",
    "build:js": "webpack",
    "clean": "rm -f -r ./www/{js,css}",
    "lint": "tslint './src/**/*.ts'",
    "start": "http-server -c-1 ./www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:css watch:js watch:cordova",
    "watch:css": "postcss -w -o ./www/css/index.css ./res/css/index.css",
    "watch:cordova": "watch 'cordova prepare' ./www",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "dependencies": {
    "@emonkak/enumerable": "^2.0.0",
    "classnames": "^2.2.5",
    "cordova-ios": "^4.4.0",
    "cordova-plugin-inappbrowser": "^1.7.1",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "fastclick": "^1.0.6",
    "fbjs": "^0.8.12",
    "intl": "^1.2.5",
    "intl-relativeformat": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "object.values": "^1.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.4",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "text-encoding": "^0.6.4",
    "tslib": "^1.7.1",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.47",
    "@types/classnames": "2.2.0",
    "@types/cordova": "0.0.34",
    "@types/cordova-plugin-inappbrowser": "^1.7.0",
    "@types/lodash.debounce": "^4.0.2",
    "@types/lodash.throttle": "^4.1.2",
    "@types/prop-types": "^15.5.1",
    "@types/query-string": "^4.3.1",
    "@types/react": "^15.0.11",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^3.0.4",
    "@types/redux-logger": "^3.0.0",
    "@types/text-encoding": "0.0.30",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "cssnano": "^3.10.0",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^4.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-csso": "^2.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^10.0.0",
    "postcss-simple-vars": "^4.0.0",
    "ts-loader": "^2.0.1",
    "tslint": "^5.0.0",
    "typescript": "^2.1.6",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "watch": "^1.0.2",
    "webpack": "^2.2.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-inappbrowser": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
