{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader for me",
  "author": "Shota Nozaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -p build:css build:js",
    "build:css": "postcss -c postcss.config.production.json -o public/css/index.css resources/css/index.css",
    "build:js": "webpack",
    "clean": "rm -f -r public/{js,css}",
    "lint": "tslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server ./public",
    "watch": "npm-run-all -p watch:css watch:js",
    "watch:css": "postcss -c postcss.config.json -w -o public/css/index.css resources/css/index.css",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "dependencies": {
    "@emonkak/enumerable": "^1.1.0",
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-overlays": "^0.6.10",
    "react-router": "^3.0.2",
    "symbol-observable": "^1.0.4"
  },
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/react": "^15.0.2",
    "@types/react-dom": "^0.14.21",
    "@types/react-router": "^2.0.46",
    "@types/whatwg-fetch": "^0.0.33",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "cssnano": "^3.10.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^2.6.0",
    "postcss-color-function": "^2.0.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "watch": "^1.0.1",
    "webpack": "^2.2.0"
  }
}
