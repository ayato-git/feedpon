{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader for me",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -p build:css build:fonts build:js",
    "build:css": "postcss -c postcss.production.config.json -o app/chrome/css/index.css src/css/index.css",
    "build:fonts": "cp -r node_modules/font-awesome/fonts app/chrome/fonts",
    "build:js": "webpack",
    "clean": "rm -fr app/chrome/{js,css,fonts}",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:css watch:fonts watch:js",
    "watch:css": "postcss -c postcss.config.json -w -o app/chrome/css/index.css src/css/index.css",
    "watch:fonts": "watch 'npm run build:fonts' node_modules/font-awesome/fonts",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "dependencies": {
    "@emonkak/enumerable": "0.0.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.6.1",
    "in-viewport": "^3.4.1",
    "localforage": "^1.4.0",
    "querystring": "^0.2.0",
    "react": "^0.14.8",
    "react-addons-pure-render-mixin": "^0.14.8",
    "react-addons-update": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-router": "^2.4.0",
    "react-waypoint": "^1.3.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.7",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "cssnano": "^3.5.2",
    "npm-run-all": "^1.8.0",
    "postcss-cli": "^2.5.2",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.2",
    "ts-loader": "^0.8.2",
    "typescript": "^1.9.0-dev.20160507",
    "typings": "^0.6.10",
    "watch": "^0.16.0",
    "webpack": "^1.13.0"
  }
}
