{
  "name": "feedpon",
  "version": "0.0.0",
  "description": "The feed reader for me",
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -p build:css build:fonts build:js",
    "build:css": "postcss -c postcss.production.config.json -o app/chrome/css/index.css src/css/index.css",
    "build:fonts": "ncp node_modules/font-awesome/fonts app/chrome/fonts",
    "build:js": "webpack --optimize-dedupe --optimize-minimize",
    "clean": "rimraf app/chrome/{js,css,fonts}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:css watch:fonts watch:js",
    "watch:css": "postcss -c postcss.config.json -w -o app/chrome/css/index.css src/css/index.css",
    "watch:fonts": "watch 'npm run build:fonts' node_modules/font-awesome/fonts",
    "watch:js": "webpack --watch --devtool source-map"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.0",
    "font-awesome": "^4.4.0",
    "in-viewport": "^3.4.1",
    "localforage": "^1.3.0",
    "lodash.difference": "^4.0.1",
    "querystring": "^0.2.0",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^2.0.0-rc5",
    "react-waypoint": "^1.1.3",
    "reflect-metadata": "^0.1.2",
    "regenerator": "^0.8.42",
    "rxjs": "^5.0.0-beta.1",
    "whatwg-fetch": "^0.10.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "css-loader": "^0.23.0",
    "cssnano": "^3.4.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.2.12",
    "postcss-cli": "^2.4.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.5.1",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.3",
    "watch": "^0.16.0",
    "webpack": "^1.12.9"
  }
}
