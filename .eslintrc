{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1,
        "semi": 2,
        "no-unused-vars": [1, {
            "vars": "all",
            "args": "none"
        }]
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
